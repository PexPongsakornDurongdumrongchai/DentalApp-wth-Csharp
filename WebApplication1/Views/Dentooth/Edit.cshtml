@model Dentooth;

@{
    ViewData["Title"] = "Edit";
}


<h1 class="text-center">Edit Dentals</h1>
<form method="POST" asp-controller="Dentooth" asp-action="Edit">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Description"></label>
        <input type="text" asp-for="Description" class="form-control" />

    </div>
    <button type="submit" class="btn btn-primary my-2">อัพเดท</button>
</form>
